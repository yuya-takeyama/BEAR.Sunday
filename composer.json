{
    "name": "BEAR.Sunday",
    "type": "library",
    "description": "BEAR.Sunday, a resource oriented framework",
    "version": "0.3.1b",
    "keywords": ["rest", "framework", "api"],
    "homepage": "https://github.com/koriym/BEAR.Sunday",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Akihito Koriyama",
            "email": "akihito.koriyama@gmail.com",
            "homepage": "https://github.com/koriym"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-apc": "*",
        "ext-curl": "*",
        "Aura/Autoload": "dev-master",
        "Aura/Di": "dev-master",
        "Aura/Router": "dev-master",
        "Aura/Signal": "dev-master",
        "Aura/Web": "dev-master",
        "BEAR/Resource": "dev-master",
        "doctrine/common": "2.3.*-dev",
        "doctrine/dbal": "2.3.*-dev",
        "facebook/xhprof": "0.9.*",
        "guzzle/guzzle": "2.7.*",
        "monolog/monolog": "1.*",
        "pagerfanta/pagerfanta": "dev-master",
        "smarty/smarty": "3.1.*",
        "symfony/console": "2.0.*",
        "symfony/http-foundation": "2.0.*",
        "Ray/Aop": "dev-master",
        "Ray/Di": "dev-master",
        "vdump/vdump": "*"
    },
    "require-dev": {
        "crodas/haanga": "dev-master",
        "twig/twig": "dev-master",
        "zend/zend-log1": "1.11",
        "zend/zend-cache1": "1.11",
        "zendframework/zend-loader": "2.0.*-dev",
        "zendframework/zend-eventmanager": "2.0.*-dev",
        "zendframework/zend-servicemanager": "2.0.*-dev",
        "zendframework/zend-cache": "2.0.*-dev",
        "zendframework/zend-log": "2.0.*-dev",
        "zendframework/zend-stdlib": "2.0.*-dev"
    },
    "autoload": {
        "psr-0": {
            "BEAR\\Sunday": "src/",
            "Aura\\Di": "vendor/Aura/Di/src/",
            "Aura\\Router": "vendor/Aura/Router/src/",
            "Aura\\Signal": "vendor/Aura/Signal/src/",
            "Aura\\Web": "vendor/Aura/Web/src/"
        }
    },
    "require-dev": {
        "crodas/haanga": "dev-master",
        "twig/twig": "dev-master"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "smarty/smarty",
                "version": "3.1.8",
                "dist": {
                    "url": "http://www.smarty.net/files/Smarty-3.1.8.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "http://smarty-php.googlecode.com/svn/",
                    "type": "svn",
                    "reference": "tags/Smarty_3_1_8/distribution/"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "crodas/haanga",
                "version": "1.0.4",
                "dist": {
                    "url": "https://github.com/crodas/Haanga/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/crodas/Haanga.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type":"package",
            "package": {
                "name": "vdump/vdump",
                "version": "0.1.0",
                "target-dir": "vdump",
                "dist": {
                    "url": "https://github.com/koriym/vdump/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/koriym/vdump.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "facebook/xhprof",
                "version": "0.9.2",
                "dist": {
                    "url": "https://github.com/facebook/xhprof/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/facebook/xhprof.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "xhprof/xhprof",
                "version": "0.9.2",
                "dist": {
                    "url": "https://github.com/facebook/xhprof/zipball/master",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/facebook/xhprof.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type":"package",
            "package": {
                "name": "zend/zend-log1",
                "version": "1.11",
                "source": {
                    "url": "http://framework.zend.com/svn",
                    "type": "svn",
                    "reference": "framework/standard/trunk/library/Zend/Log/"
                },
                "target-dir": "Zend/Log",
                "autoload": {
                    "psr-0": { "Zend_Log": "/" }
                }
            }
        },
        {
            "type":"package",
            "package": {
                "name": "zend/zend-cache1",
                "version": "1.11",
                "source": {
                    "url": "http://framework.zend.com/svn",
                    "type": "svn",
                    "reference": "framework/standard/trunk/library/Zend/Cache/"
                },
                "target-dir": "Zend/Cache",
                "autoload": {
                    "psr-0": { "Zend_Cache": "/" }
                }
            }
        }
    ]
}
